[Distribution]
Distribution=fedora
Architecture=x86-64
Release=rawhide
RepositoryKeyCheck=no
Mirror=https://mirror.sjtu.edu.cn/fedora
[Build]
ToolsTreeRelease=rawhide
ToolsTreeMirror=https://mirror.sjtu.edu.cn/fedora
History=yes
CacheDirectory=mkosi.cache
Incremental=yes
ToolsTree=default
[Output]
Format=disk
OutputDirectory=mkosi.output
ImageId=ParticleOS
SplitArtifacts=no
[Content]
Bootable=yes
UnifiedKernelImages=yes
UnifiedKernelImageFormat=%i_%v
KernelInitrdModules=*
RootPassword=11112222
BaseTrees=%O/base
KernelCommandLine=rw logo.nologo security=none audit=0 mitigations=off
[Runtime]
RuntimeSize=30G
RAM=4G
CPUs=4
TPM=no
[Validation]
SecureBoot=true

